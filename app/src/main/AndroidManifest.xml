        <activity android:name=".RegisterActivity" />
        <activity android:name=".Set_Activity" />
        <activity android:name=".ArrayAdapter_ListView_test"/>
        <activity android:name=".SimpleAdapter_ListView_test"/>

        <activity android:name=".BaseAdapter_ListView_test"
            android:label="BaseListView">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>
</manifest>